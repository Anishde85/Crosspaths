                </div>

                <div id="button-wrapper">
                    <div id="button-containers">
                        <a href="/join-meeting">Join Meeting</a>
                    </div>
                </div>
            </div>
            <div id="right-part">
                <div id="gif">
                    <img src = "/assets/msteams-gif.gif" alt=""/>
                </div>
            </div>    
        </div>
    </div>
    <script type="text/javascript" src="glass.js"></script>
        <script>
            VanillaTilt.init(document.querySelectorAll("#button-wrapper"), {
                max: 25,
                speed: 400,
                glare: true,
                "max-glare": 1,
            });
        </script>
